{
  "name": "node-scc-client",
  "version": "1.0.0",
  "author": "ZEXSM",
  "private": true,
  "description": "Decrypting data encrypted in Spring Cloud Config",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint --ext .ts src test",
    "example": "ts-node-dev --respawn --transpile-only example/server.ts",
    "prestart": "npm run build",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZEXSM/node-scc-client"
  },
  "keywords": [
    "nodejs",
    "server",
    "typescript",
    "decrypt",
    "sping-cloud"
  ],
  "dependencies": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/jest": "26.0.23",
    "@types/node": "15.0.1",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "7.25.0",
    "express": "4.17.1",
    "jest": "26.6.3",
    "ts-jest": "26.5.5",
    "ts-node-dev": "1.1.6",
    "typescript": "4.2.4"
  },
  "files": [
    "src/**/*"
  ]
}
